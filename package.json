    "react-autosuggest": "^9.3.2",
    "react-dates": "^12.6.0",
    "react-facebook-login-component": "^0.9.0",
    "react-i18n-q": "^1.0.5",
    "react-paginate": "^4.4.4",
    "react-select": "^1.0.0-rc.10",
    "react-slick": "^0.15.4",
    "recharts": "^1.0.0-alpha.4",
    "redux-thunk": "^2.2.0",
    "replace-in-file": "^3.0.0-beta.2"
  },
  "peerDependencies": {
    "lodash": "^4.17.4",
    "moment": "^2.18.1",
    "react": "^15.6.1",
    "react-apollo": "^1.4.12",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "recompose": "^0.25.0",
    "redux": "^3.7.2",
    "styled-components": "^2.1.2"
  },
