{
  "name": "avantrip-react",
  "version": "1.0.4",
  "description": "Avantrip React Components",
  "main": "/dist/index.js",
  "scripts": {
    "build-storybook": "build-storybook -s ./statics",
    "profile": "export PROFILE=true && build-storybook -s ./statics",
    "copysrc": "rimraf dist && cp -r src dist && npm run copypackage",
    "copypackage": "cp -rf package.json dist",
    "copybabelrc": "cp .storybook/.babelrc ./",
    "copyreadme": "cp README.md dist",
    "build": "rimraf dist && npm run copybabelrc && babel src -d dist && npm run copypackage && npm run copyreadme",
    "build-old": "rimraf dist && webpack -p && npm run build-storybook &&  npm run copypackage",
    "start": "node server",
    "dev": "npm run storybook",
    "test": "...",
    "storybook": "start-storybook -p 9001 -s ./statics",
    "dist": "npm run build && npm version patch && npm run copypackage && cd dist && npm publish",
    "gen": "plop",
    "gentest": "rimraf src/global/test && plop",
    "peers": "npm run npm-install-peers"
  },
  "repository": {
    "type": "git",
    "url": "git@repo.avantrip.com:mzapaia/ui-components.git"
  },
  "author": "",
  "license": "ISC",
